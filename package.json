{
  "name": "rentalx",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "babel dataSource.ts src --extensions \".js,.ts\" --out-dir dist --copy-files",
    "buildtsc": "tsc",
    "dev": "ts-node-dev --inspect --transpile-only --ignore-watch node_modules --respawn ./src/shared/infra/http/server.ts",
    "migration:generate": "typeorm-ts-node-commonjs -d ./dataSource.ts migration:generate ./src/shared/infra/typeorm/migrations/",
    "migration:run": "typeorm-ts-node-commonjs -d ./dataSource.ts migration:run",
    "migration:revert": "typeorm-ts-node-commonjs -d ./dataSource.ts migration:revert",
    "migration:create": "typeorm-ts-node-commonjs migration:create ",
    "test": "NODE_ENV=test jest --runInBand --detectOpenHandles",
    "seed:admin": "ts-node-dev src/shared/infra/typeorm/seed/admin.ts"
  },
  "dependencies": {
    "aws-sdk": "^2.1236.0",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.5.1",
    "csv-parse": "^5.2.0",
    "dayjs": "^1.11.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "handlebars": "^4.7.7",
    "jsonwebtoken": "^8.5.1",
    "mime": "^3.0.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.8.0",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "supertest": "^6.2.4",
    "swagger-ui-express": "^4.4.0",
    "tsyringe": "^4.7.0",
    "typeorm": "^0.3.7",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.19.6",
    "@babel/node": "^7.19.1",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.19.6",
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-typescript": "^7.18.6",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.6",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/mime": "^3.0.1",
    "@types/multer": "^1.4.7",
    "@types/nodemailer": "^6.4.6",
    "@types/node": "^18.7.23",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "eslint": "^8.17.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-import-helpers": "^1.2.1",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "26.6.3",
    "prettier": "^2.6.2",
    "ts-jest": "26.5.4",
